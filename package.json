{
  "name": "project-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "development": "vue-tsc --noEmit && vite build --mode development",
    "staging": "vue-tsc --noEmit && vite build --mode staging",
    "production": "vue-tsc --noEmit && vite build --mode production",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore src",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "validate": "npm-run-all --parallel lint-staged"
  },
  "lint-staged": {
    "*.+(ts|vue)": [
      "npm run lint",
      "tsc-files --noEmit"
    ]
  },
  "dependencies": {
    "@appwise/vue-oauth2": "^0.0.23",
    "@vue/runtime-core": "^3.2.39",
    "@wouterlms/composables": "^0.2.18",
    "@wouterlms/forms": "^0.2.20",
    "@wouterlms/ui": "^1.0.3",
    "axios": "^1.1.3",
    "axios-cache-adapter": "^2.7.3",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@wouterlms/create-route-map": "^1.0.3",
    "@wouterlms/icons": "^0.0.3",
    "@wouterlms/svg-transformer": "^1.0.6",
    "autoprefixer": "^10.4.10",
    "eslint": "^8.23.1",
    "eslint-config-appwise": "^1.0.6",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-config-wouter": "^0.0.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-putout": "^16.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "putout": "^27.8.0",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.8",
    "tailwindcss-theme-swapper": "^0.6.2",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.0-dev.20220925",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.8",
    "vite-plugin-checker": "^0.5.1",
    "vue-tsc": "^1.0.7"
  }
}
